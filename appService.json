{
    "label": "google-drive-easy",
    "name": "google-drive-easy",
    "apiVersion": "v1",
    "internal": false,
    "configurationType": "GLOBAL",
    "deployment": {
        "jsService": true,
        "profiles":[
            {
                "name": "small",
                "label": "Small",
                "memory": 128,
                "description": "Small Memory Allocation"
            },
            {
                "name": "default",
                "label": "Default",
                "memory": 256,
                "description": "Default Memory Allocation"
            },
            {
                "name": "medium",
                "label": "Medium",
                "memory": 512,
                "description": "Medium Memory Allocation"
            },
            {
                "name": "big",
                "label": "Big",
                "memory": 1024,
                "description": "Big Memory Allocation"
            }
        ],
        "allowMultipleInstances": false
    },
    "status": "ACTIVE",
    "stores": [],
    "events": [
        {
            "label": "On file list completed",
            "name": "onFileListComplete",
            "description": "Callback event for when the file list has been retrieved"
        }
    ],
    "functions": [
        {
            "label": "get shared folder files list",
            "name": "getFileInSharedFolder",
            "description": "The service receives a folder ID and returns the list of files in the shared folder",
            "callbacks": [
                {
                    "name": "onFileListComplete",
                    "maxWaitingTime": 120000,
                    "maxExpectedResponses": 1
                }
            ]
        }
    ],
    "configuration":[]
}